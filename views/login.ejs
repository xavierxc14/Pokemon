<div class="col-md-4 col-md-offset-4">
    <form action="auth/login" method="post" class="form-signin">
        <h2 class="form-signin-heading">Ingrese sus datos:</h2>
        <label for="usuario" class="sr-only">Usuario:</label>
        <input id="usuario" name="usuario" type="text" class="form-control" placeholder="Usuario" required="true"
               autofocus=true"">
        <label for="password" class="sr-only">Password:</label>
        <input id="password" name="password" type="password" class="form-control" placeholder="Password"
               required="true">
        <input type="submit" class="btn btn-lg btn-primary btn-block" value="Ingresar">
    </form>
</div>
